<h2><%= title %></h2>

<hr>

<% if (!data || data.length === 0) { %>
  <p>No data available</p>
<% } else { %>

<table border="1" cellpadding="10">
  <thead>
    <tr>
      <% Object.keys(data[0]).forEach(key => { %>
        <th><%= key %></th>
      <% }) %>
    </tr>
  </thead>

  <tbody>
    <% data.forEach(row => { %>
      <tr>
        <% Object.values(row).forEach(value => { %>
          <td><%= value %></td>
        <% }) %>
      </tr>
    <% }) %>
  </tbody>
</table>

<% } %>

<br><br>
<a href="/admin/dashboard">Back to Dashboard</a>